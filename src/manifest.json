{
    "name": "Sniffer",
    "description": "",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "webRequest",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_icon": {
            "128": "sniffer.png"
        }
    },
    "icons": {
        "128": "sniffer.png"
    },
    "options_ui": {
        "page": "index.html",
        "open_in_tab": true
    }
}
